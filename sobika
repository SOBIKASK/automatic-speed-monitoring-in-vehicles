#include <stdio.h>
 
/*structure declaration*/
struct employee{
    char    name[30];
    int     age;
    long int phonenumber ;
    float   salary;
};
 
int main()
{
    /*declare structure variable*/
    struct employee emp;
     
    /*read employee details*/
   
    printf("Name :");          gets(emp.name);
    printf("age:");            scanf("%d",&emp.age);
    printf("phone number:");   scanf("%d",&emp.phonenumber);
    printf("Salary :\n\n");        scanf("%f",&emp.salary);
     
  printf(" first employee Name :%s\n",emp.name);        
    printf("age:%d\n",emp.age);            
    printf("phone number:%d\n",emp.phonenumber);  
    printf("Salary :%f\n",emp.salary);   
    
     /*read employee details*/
   
    printf("\nName : ");         scanf("%s",&emp.name);
    printf("age: ");            scanf("%d",&emp.age);
    printf("phone number:") ;  scanf("%d",&emp.phonenumber);
    printf("Salary :");        scanf("%f",&emp.salary);
    
    printf("secound employee Name :%s\n",emp.name);        
    printf("age:%d\n",emp.age);            
    printf("phone number:%d\n",emp.phonenumber);  
    printf("Salary :%f",emp.salary);   
    
   
    return 0;
}
